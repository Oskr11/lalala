    <script>
        // Обновление времени в реальном времени
        setInterval(function() {
            var daysElement = document.getElementById("days");
            var hoursElement = document.getElementById("hours");
            var minutesElement = document.getElementById("minutes");
            var secondsElement = document.getElementById("seconds");

            // Уменьшаем время на 1 секунду
            var days = parseInt(daysElement.textContent);
            var hours = parseInt(hoursElement.textContent);
            var minutes = parseInt(minutesElement.textContent);
            var seconds = parseInt(secondsElement.textContent);

            if (days <= 0 && hours <= 0 && minutes <= 0 && seconds <= 0) {
                clearInterval(interval); // Остановить интервал обновления времени
                document.getElementById("message").style.display = "block"; // Показать текст сообщения
                return; // Выходим из функции
            }

            seconds--;

            if (seconds < 0) {
                minutes--;
                seconds = 59;
            }
            if (minutes < 0) {
                hours--;
                minutes = 59;
            }
            if (hours < 0) {
                days--;
                hours = 23;
            }

            // Обновляем отображаемое время
            daysElement.textContent = days;
            hoursElement.textContent = hours;
            minutesElement.textContent = minutes;
            secondsElement.textContent = seconds;

        
        }, 1000); // Каждую секунду
    </script>
        
    
